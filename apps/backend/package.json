{
  "name": "@starter-nuxt-amplify-saas/backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "pnpm exec ampx pipeline-deploy --branch $AWS_BRANCH --app-id $AWS_APP_ID",
    "sandbox:init": "pnpm exec ampx sandbox",
    "sandbox:delete": "pnpm exec ampx sandbox delete",
    "sandbox:secrets": "printf %s \"$STRIPE_SECRET_KEY\" | pnpm exec ampx sandbox secret set STRIPE_SECRET_KEY",
    "sandbox:seed": "pnpm exec ampx sandbox seed",
    "sandbox:seed:plans": "SEED_TASK=plans pnpm exec ampx sandbox seed",
    "sandbox:seed:users": "SEED_TASK=users pnpm exec ampx sandbox seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@aws-amplify/backend": "^1.16.1",
    "@aws-amplify/backend-cli": "^1.8.0",
    "@types/aws-lambda": "^8.10.145",
    "aws-cdk-lib": "2.204.0",
    "constructs": "^10.0.0",
    "esbuild": "^0.25.6",
    "tsx": "^4.20.3",
    "@aws-amplify/seed": "^1.0.0",
    "stripe": "^17.7.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "aws-amplify": "^6.15.3",
    "zod": "3.25.17",
    "@starter-nuxt-amplify-saas/billing": "workspace:*"
  }
