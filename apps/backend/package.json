{
  "name": "@starter-nuxt-amplify-saas/backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "exports": {
    "./schema": "./amplify/data/resource.ts"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "pnpm exec ampx pipeline-deploy --branch $AWS_BRANCH --app-id $AWS_APP_ID",
    "sandbox:init": "pnpm exec ampx sandbox",
    "sandbox:delete": "pnpm exec ampx sandbox delete",
    "sandbox:secrets": "printf %s \"$STRIPE_SECRET_KEY\" | pnpm exec ampx sandbox secret set STRIPE_SECRET_KEY",
    "seed": "pnpm exec ampx sandbox seed",
    "seed:plans": "SEED_TASK=plans pnpm exec ampx sandbox seed",
    "seed:users": "SEED_TASK=users pnpm exec ampx sandbox seed",
    "seed:stripe": "stripe fixtures amplify/seed/data/stripe.json --api-key $STRIPE_SECRET_KEY",
    "sandbox:amplify:seed": "tsx scripts/amplify-seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@aws-amplify/backend": "^1.18.0",
    "@aws-amplify/backend-cli": "^1.8.0",
    "@aws-amplify/seed": "^1.1.0",
    "@types/aws-lambda": "^8.10.159",
    "aws-cdk-lib": "2.204.0",
    "constructs": "^10.4.3",
    "esbuild": "^0.25.12",
    "stripe": "^17.7.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "aws-amplify": "^6.15.8",
    "zod": "3.25.17"
  }
}
