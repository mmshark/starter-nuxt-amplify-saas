    "@starter-nuxt-amplify-saas/i18n": "workspace:*",
    "@starter-nuxt-amplify-saas/uix": "workspace:*",
    "@stripe/stripe-js": "^4.10.0",
    "aws-amplify": "^6.15.8",
    "jsonwebtoken": "^9.0.2",
    "stripe": "^17.7.0"
  },
  "devDependencies": {
    "@aws-amplify/seed": "^1.1.0",
    "@nuxt/eslint": "latest",
    "@types/jsonwebtoken": "^9.0.10",
    "@vueuse/core": "^13.9.0",
    "eslint": "^9.39.1",
    "nuxt": "^4.2.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vue": "latest"
  }
}
