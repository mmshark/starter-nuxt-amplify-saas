{
  "name": "@starter-nuxt-amplify-saas/billing",
  "type": "module",
  "version": "0.0.1",
  "main": "./nuxt.config.ts",
  "exports": {
    ".": "./nuxt.config.ts",
    "./utils": "./utils/index.ts",
    "./types": "./types/index.ts",
    "./composables/*": "./composables/*"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
