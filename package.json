{
  "name": "starter-nuxt-amplify-saas",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "layers/*"
  ],
  "scripts": {
    "backend:sandbox:init": "pnpm --filter @starter-nuxt-amplify-saas/backend run sandbox:init",
    "backend:sandbox:delete": "pnpm --filter @starter-nuxt-amplify-saas/backend run sandbox:delete -y",
    "backend:sandbox:secrets": "pnpm --filter @starter-nuxt-amplify-saas/backend run sandbox:secrets",
    "backend:sandbox:seed": "pnpm --filter @starter-nuxt-amplify-saas/backend run sandbox:seed",
    "backend:sandbox:seed:plans": "pnpm --filter @starter-nuxt-amplify-saas/backend run sandbox:seed:plans",
    "backend:sandbox:seed:users": "pnpm --filter @starter-nuxt-amplify-saas/backend run sandbox:seed:users",
    "amplify:sandbox:generate-outputs": "pnpm --filter @starter-nuxt-amplify-saas/amplify run sandbox:generate-outputs",
    "amplify:sandbox:generate-graphql-client-code": "pnpm --filter @starter-nuxt-amplify-saas/amplify run sandbox:generate-graphql-client-code",
    "saas:dev": "pnpm --filter @starter-nuxt-amplify-saas/saas run dev",
    "saas:build": "pnpm --filter @starter-nuxt-amplify-saas/saas run build",
    "landing:dev": "pnpm --filter @starter-nuxt-amplify-saas/landing run dev",
    "landing:build": "pnpm --filter @starter-nuxt-amplify-saas/landing run build",
    "landing:generate": "pnpm --filter @starter-nuxt-amplify-saas/landing run generate",
    "amplify:generate-graphql-client-code": "pnpm --filter @starter-nuxt-amplify-saas/amplify run generate-graphql-client-code",
    "amplify:generate-outputs": "pnpm --filter @starter-nuxt-amplify-saas/amplify run generate-outputs",
    "billing:stripe:sync": "tsx scripts/billing-stripe-sync.ts layers/billing/billing-plans.json",
    "billing:stripe:login": "stripe login",
    "billing:stripe:listen": "pnpm --filter @starter-nuxt-amplify-saas/billing run stripe:listen",
    "saas:test:auth": "pnpm --filter @starter-nuxt-amplify-saas/saas run test:auth",
    "saas:test:auth:headed": "pnpm --filter @starter-nuxt-amplify-saas/saas run test:auth:headed",
    "saas:test:auth:setup": "pnpm --filter @starter-nuxt-amplify-saas/saas run test:auth:setup"
  },
  "devDependencies": {
    "@aws-amplify/backend-cli": "^1.8.0",
    "dotenv": "^17.2.2",
    "stripe": "^17.7.0",
    "tsx": "^4.20.5"
  },
  "packageManager": "pnpm@10.13.1",
  "pnpm": {
    "overrides": {
      "vue": "3.5.21",
      "@vue/server-renderer": "3.5.21"
    }
  }
}
